{
    "compilerOptions": {
      "outDir": "./dist",
      "target": "ESNext",
      "module": "esnext",
      "rootDir": "./src",
      "moduleResolution": "node",
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": true,
      "alwaysStrict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "skipLibCheck": true
    }
  }